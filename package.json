{
    "name": "tmdb-test",
    "version": "1.0.0",
    "description": "Node js tmdb",
    "main": "index.js",
    "type": "module",
    "esm": "cjs",
    "events": {
        "restart": "npm run build",
        "start": "npm run build"
    },
    "scripts": {
        "clean": "cleandir dist",
        "prebuild": "npm run clean",
        "build": "babel . -d dist --ignore \"node_modules/*\" --ignore \"test/*\" --out-dir dist",
        "prestart-dev": "npm run build",
        "serve": "nodemon --watch app --watch config --watch view --watch index.js  --exec \"npm run build && babel-node dist/index.js\"",
        "start": "nodemon --exec babel-node  index.js",
        "test": "mocha --require @babel/register --require babel-polyfill --timeout 30000 --exit"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "keywords": [
        "node base",
        "es6"
    ],
    "author": "Chanchal Rajbanshi",
    "license": "ISC",
    "dependencies": {
        "@hapi/joi": "^16.1.8",
        "amqp": "^0.2.7",
        "amqplib": "^0.5.6",
        "axios": "^0.19.2",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.0",
        "colors": "^1.4.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "detect-mocha": "^0.1.0",
        "dotenv": "^8.1.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "express-logger": "0.0.3",
        "helmet": "^3.23.3",
        "http-status": "^1.4.2",
        "js-md5": "^0.7.3",
        "jsonwebtoken": "^8.5.1",
        "log4js": "^5.1.0",
        "method-override": "^3.0.0",
        "mongoose": "^5.9.28",
        "mongoose-unique-validator": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "redis": "^2.8.0",
        "request": "^2.88.2",
        "socket.io": "^2.3.0",
        "superagent": "^6.0.0",
        "swagger-ui-express": "^4.1.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.1",
        "@babel/node": "^7.10.5",
        "@babel/preset-env": "^7.11.0",
        "@babel/register": "^7.10.5",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.1.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "clean-dir": "^1.0.3",
        "mocha": "^6.2.3",
        "nodemon": "^2.0.4",
        "watch": "^1.0.2"
    },
    "options": {
        "babelrc": true
    }
}
